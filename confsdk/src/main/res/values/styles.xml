<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/white</item>
        <!--<item name="colorPrimary">@color/video_item</item>-->
        <!--<item name="colorPrimaryDark">@color/video_item</item>-->
        <!--<item name="colorAccent">@color/video_item</item>-->
    </style>

    <!-- 手机样式选择 -->
    <style name="meeting_header" parent="@style/fw">
        <item name="android:background">@drawable/list_toptool_bg</item>
    </style>

    <style name="meeting_header_title" parent="@style/ww">
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">@dimen/textSizeMid</item>
    </style>

    <style name="metting_list_input" parent="@style/fw">
        <item name="android:background">@drawable/meeting_list_input</item>
        <item name="android:layout_marginLeft">@dimen/width_13_80</item>
        <item name="android:layout_marginRight">@dimen/width_10_80</item>
    </style>

    <!-- ActionBar -->
    <style name="styleActionBar" parent="@style/fw">
        <item name="android:layout_height">@dimen/height_6_80</item>
        <item name="android:background">@color/actionbar_bg</item>
    </style>

    <!-- ActionBar Mid -->
    <style name="styleActiobBar_mid" parent="@style/ww">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- conferencelist -->
    <style name="styleActionBar_midbutton" parent="@style/ww">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/textSize_05</item>
    </style>

    <style name="styleText" parent="@style/ww">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@drawable/contactcommon_color_on</item>
    </style>

    <style name="styleText_confitem" parent="@style/ww">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/confitem_color_on</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="styleInput" parent="@style/fw">
        <item name="android:layout_marginLeft">@dimen/width_7_80</item>
        <item name="android:layout_marginRight">@dimen/width_7_80</item>
    </style>

    <!-- Layout Horizontal use -->
    <style name="styleLayout_h" parent="@style/styleInput">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/edit_foucs_on</item>
        <item name="android:addStatesFromChildren">true</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- PreConf EditText use -->
    <style name="stylePreConf_edit" parent="@style/styleInput">
        <item name="android:paddingLeft">@dimen/width_4_80</item>
        <item name="android:paddingRight">@dimen/width_2_80</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/textSize_04</item>
        <item name="android:background">@drawable/edit_foucs_on</item>
    </style>

    <!-- PreConf Button -->
    <style name="stylePreConf_button" parent="@style/styleInput">
        <item name="android:layout_marginTop">@dimen/height_5_80</item>
    </style>

    <!-- Site Check Dialog -->
    <style name="styleSiteCheckDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:background">#00000000</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <!-- PreConf Create Left use -->
    <style name="stylePen" parent="@style/ww">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <!-- PreConf Create Left use -->
    <style name="styleLayout_leftItem" parent="@style/ww">
        <item name="android:textColor">@color/actionbar_bg</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!-- PreConf Create Right use -->
    <style name="styleLayout_rightItem" parent="@style/fw">
        <item name="android:layout_marginLeft">@dimen/width_2_80</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="styleLayout_rightImg" parent="@style/ww">
        <item name="android:layout_marginRight">@dimen/width_1_80</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!-- conference -->
    <style name="foot_bar">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="styleLayout_time" parent="@style/ww">
        <item name="android:layout_marginTop">@dimen/height_3_80</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/bg_time</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/textSize_02</item>
    </style>

    <!-- 工具栏 -->
    <style name="StyleToolBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <!-- 登陆信息文本框 -->
    <style name="StyleLoginEditor">
        <item name="android:textSize">@dimen/textSizeSmall</item>
        <item name="android:textColor">@color/loginContent</item>
        <item name="android:layout_marginTop">18px</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textScaleX">1</item>
        <item name="android:textColorHint">@color/loginContentTip</item>
        <item name="android:paddingLeft">10px</item>
        <item name="android:paddingRight">10px</item>
        <item name="android:background">@drawable/a6_et_common</item>
    </style>
    <!-- InputDelete -->
    <style name="input_delete" parent="@style/ww">
        <item name="android:visibility">gone</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@color/backgroudLiqid</item>
        <item name="android:src">@drawable/input_delete</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>
    <!-- 加入会议按钮 -->
    <!--<style name="StyleLoginButton" parent="@style/fw">-->
    <!--<item name="android:layout_marginTop">20px</item>-->
    <!--<item name="android:background">@drawable/meeting_list_button_sel</item>-->
    <!--<item name="android:textColor">@color/white</item>-->
    <!--<item name="android:textSize">@dimen/textSizeSmall</item>-->
    <!--</style>-->
    <!-- ui.notice by Jack.Yan -->
    <style name="dialog_title" parent="fw">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:shadowColor">@android:color/black</item>
        <item name="android:shadowDx">2</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/infobg01</item>
        <item name="android:text">@string/dialog_title</item>
    </style>

    <style name="dialog_message" parent="fw">
        <item name="android:layout_height">85dp</item>
        <item name="android:background">@drawable/infobg02</item>
        <item name="android:padding">25dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="dialog_button">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="style_common_input_and_btn" parent="@style/fw">
        <item name="android:layout_marginLeft">@dimen/height_5_80</item>
        <item name="android:layout_marginRight">@dimen/height_5_80</item>
    </style>

    <style name="style_rl_titleBar" parent="@style/fw">
        <item name="android:layout_height">@dimen/height_6_80</item>
        <item name="android:background">@color/app_main_hue</item>
    </style>

    <style name="style_rl_titleBar2" parent="@style/fw">
        <item name="android:layout_height">@dimen/height_6_80</item>
        <item name="android:background">@color/app_inconf_hue</item>
    </style>

    <style name="style_tv_titleBar_mid" parent="@style/ww">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">@dimen/textSize_05</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="style_btn_titleBar_midbutton" parent="@style/ff">
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/textSize_02</item>
    </style>

    <style name="style_btn_titleBar_rightbutton" parent="@style/ww">
        <item name="android:layout_height">@dimen/height_6_80</item>
        <item name="android:layout_width">@dimen/height_6_80</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_btn_ok" parent="@style/style_common_input_and_btn">
        <item name="android:layout_height">@dimen/height_5_80</item>
        <item name="android:background">@drawable/a6_btn_ok</item>
        <item name="android:textSize">@dimen/textSize_07</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_btn_cnl" parent="@style/style_common_input_and_btn">
        <item name="android:layout_height">@dimen/height_5_80</item>
        <item name="android:background">@drawable/a6_btn_cnl</item>
        <item name="android:textSize">@dimen/textSize_07</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_btn_gray" parent="@style/style_common_input_and_btn">
        <item name="android:layout_height">@dimen/height_5_80</item>
        <item name="android:background">@drawable/a6_btn_gray</item>
        <item name="android:textSize">@dimen/textSize_07</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_rl_input" parent="@style/style_common_input_and_btn">
        <item name="android:layout_height">@dimen/height_5_80</item>
        <item name="android:background">@drawable/a6_et_common</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="style_ll_input" parent="@style/style_common_input_and_btn">
        <item name="android:layout_height">@dimen/height_5_80</item>
        <item name="android:background">@drawable/a6_et_common</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/width_2_80</item>
        <item name="android:paddingRight">@dimen/width_2_80</item>
    </style>

    <style name="style_iv_inputicon_square" parent="@style/ww">
        <item name="android:layout_height">@dimen/width_4_80</item>
        <item name="android:layout_width">@dimen/width_4_80</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="style_et_input" parent="@style/ff">
        <item name="android:layout_marginLeft">@dimen/width_2_80</item>
        <item name="android:dropDownHeight">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:ellipsize">none</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/textSize_04</item>
        <item name="android:maxLength">64</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/a6_input_black</item>
        <item name="android:textColorHint">@color/a6_input_hint</item>
    </style>

    <style name="style_dialog_loading" parent="android:style/Theme.Dialog">

        <!-- 背景颜色及和透明程度 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 是否去除标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 是否去除边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否模糊 -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:background">#00000000</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="style_dialog_normal" parent="android:style/Theme.Dialog">

        <!-- 背景颜色及和透明程度 -->
        <item name="android:windowBackground">@drawable/a6_bg_white_corners</item>
        <!-- 是否去除标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 是否去除边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>

    <style name="style_dialog_bottom" parent="@android:style/Theme.Dialog">

        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 浮于Activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog以外的区域模糊效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- Dialog进入及退出动画 -->
        <item name="android:windowAnimationStyle">@style/anim_dialog_bottom</item>
    </style>

    <style name="anim_dialog_bottom" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/anim_in_bottomdialog</item>
        <item name="android:windowExitAnimation">@anim/anim_out_bottomdialog</item>
    </style>

    <style name="style_chat_time" parent="@style/ww">
        <item name="android:layout_marginTop">@dimen/height_101_80</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/a6_txt_black4</item>
        <item name="android:textSize">@dimen/textSize_01</item>
    </style>

    <style name="Style_ann_bar">
        <item name="android:layout_width">@dimen/width_6_80</item>
        <item name="android:layout_height">@dimen/width_6_80</item>
        <item name="android:layout_marginTop">@dimen/width_100_80</item>
        <item name="android:layout_marginBottom">@dimen/width_100_80</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="Style_ann_bar2">
        <item name="android:layout_width">@dimen/width_3_80</item>
        <item name="android:layout_height">@dimen/width_3_80</item>
        <item name="android:layout_marginTop">@dimen/width_100_80</item>
        <item name="android:layout_marginBottom">@dimen/width_100_80</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
