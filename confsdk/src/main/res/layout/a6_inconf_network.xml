<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ffv"
    android:background="@android:color/transparent"
    android:gravity="center">

    <LinearLayout
        android:id="@+id/dialog_network_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:minHeight="@dimen/dp_47"
        android:orientation="vertical"
        android:padding="@dimen/dp_11"
        android:gravity="center_horizontal"
        android:background="@drawable/a6_bg_white_corners">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/textSize_03"
            android:textColor="@color/a6_txt_black2"
            android:text="@string/inconf_network_1" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_11"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/dialog_network_tv_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minEms="5"
                android:gravity="center"
                android:textSize="@dimen/textSize_03"
                android:textColor="@color/a6_txt_blue1"
                android:text="@string/inconf_network_2" />

            <TextView
                android:id="@+id/dialog_network_tv_confirm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minEms="5"
                android:gravity="center"
                android:textSize="@dimen/textSize_03"
                android:textColor="@color/a6_txt_blue1"
                android:text="@string/inconf_network_3" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>