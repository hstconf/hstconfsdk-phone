<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_pop_index5_root"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_pop_index5_nor"
    android:orientation="vertical">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ll_pop_index5_root_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:orientation="horizontal"
        >

        <LinearLayout
            android:id="@+id/ll_pop_index5_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_vs"
            android:gravity="center"
            android:minHeight="@dimen/height_105_80"
            android:orientation="vertical"
            >

            <ImageView
                android:id="@+id/iv_pop_index5_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitCenter"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:src="@drawable/ic_pop_index5_info_nor" />

            <TextView
                android:id="@+id/tv_pop_index5_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/index_5_1"
                android:textColor="@color/index_white"
                android:textSize="12sp"
                android:layout_marginTop="@dimen/dp_2"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_pop_index5_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_vs"
            android:gravity="center"
            android:minHeight="@dimen/height_105_80"
            android:layout_marginLeft="15dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_pop_index5_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitCenter"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:src="@drawable/ic_pop_index5_set_nor" />

            <TextView
                android:id="@+id/tv_pop_index5_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/index_5_2"
                android:textColor="@color/index_white"
                android:textSize="12sp"
                android:layout_marginTop="@dimen/dp_2"
                />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_pop_index5_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_vs"
            android:gravity="center"
            android:layout_marginLeft="15dp"
            android:minHeight="@dimen/height_105_80"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_pop_index5_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitCenter"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:src="@drawable/ic_vs_chat" />

            <TextView
                android:id="@+id/tv_pop_index5_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/index_5_5"
                android:textColor="@color/index_white"
                android:textSize="12sp"
                android:layout_marginTop="@dimen/dp_2"
                />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ll_pop_index5_root_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="15dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_pop_index5_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_vs"
            android:gravity="center"
            android:minHeight="@dimen/height_105_80"
            android:layout_marginRight="15dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_pop_index5_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitCenter"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:src="@drawable/ic_pop_index5_rec_nor" />

            <TextView
                android:id="@+id/tv_pop_index5_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/index_5_3"
                android:textColor="@color/index_white"
                android:textSize="12sp"
                android:layout_marginTop="@dimen/dp_2"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_pop_index5_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_vs"
            android:gravity="center"
            android:minHeight="@dimen/height_105_80"

            android:orientation="vertical">

            <ImageView
                android:id="@+id/iv_pop_index5_0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitCenter"
                android:layout_marginLeft="@dimen/dp_6"
                android:layout_marginRight="@dimen/dp_6"
                android:src="@drawable/ic_vs_sync" />

            <TextView
                android:id="@+id/tv_pop_index5_0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/index_5_0"
                android:textColor="@color/index_white"
                android:textSize="12sp"
                android:layout_marginTop="@dimen/dp_2"/>
        </LinearLayout>


    </LinearLayout>


</LinearLayout>
